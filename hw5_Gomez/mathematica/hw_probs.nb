Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "verification", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"len", "=", "8"}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "This", " ", "should", " ", "be", " ", "the", " ", "length", " ", "of", 
     " ", "x1", " ", "and", " ", "x2"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"n", "=", 
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", 
       RowBox[{"len", "-", "1"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"x1", "=", 
     RowBox[{"Sin", "[", 
      RowBox[{"2", "*", "Pi", "*", 
       RowBox[{"n", "/", "len"}]}], "]"}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{"Your", " ", "first", " ", "signal"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"x2", "=", 
     RowBox[{"Sin", "[", 
      RowBox[{"2", "*", "Pi", "*", 
       RowBox[{"n", "/", "len"}]}], "]"}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Your", " ", "second", " ", "signal"}], ",", 
     RowBox[{
     "could", " ", "be", " ", "any", " ", "other", " ", "function"}]}], 
    "*)"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"X1", "=", 
    RowBox[{"Fourier", "[", 
     RowBox[{"x1", ",", 
      RowBox[{"FourierParameters", "->", 
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "1"}]}], "}"}]}]}], "]"}]}], "\n", 
   RowBox[{"X2", "=", 
    RowBox[{"Conjugate", "[", 
     RowBox[{"Fourier", "[", 
      RowBox[{"x2", ",", 
       RowBox[{"FourierParameters", "->", 
        RowBox[{"{", 
         RowBox[{"1", ",", 
          RowBox[{"-", "1"}]}], "}"}]}]}], "]"}], "]"}]}], "\n", 
   RowBox[{"R", "=", 
    RowBox[{"X1", "*", "X2"}]}], " ", 
   RowBox[{"(*", 
    RowBox[{"Pointwise", " ", "multiplication"}], "*)"}], "\n", 
   RowBox[{"r", "=", 
    RowBox[{"InverseFourier", "[", 
     RowBox[{"R", ",", 
      RowBox[{"FourierParameters", "->", 
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "1"}]}], "}"}]}]}], "]"}]}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "Inverse", " ", "DFT", " ", "to", " ", "get", " ", "the", " ", "circular",
      " ", "correlation"}], "*)"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"r", "=", 
    RowBox[{"Simplify", "[", "r", "]"}]}], "\n", 
   RowBox[{"ListPlot", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Tooltip", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Re", "[", "#1", "]"}], ",", 
           RowBox[{"Im", "[", "#1", "]"}]}], "}"}], "]"}], "&"}], ")"}], "/@", 
      RowBox[{"N", "[", "r", "]"}]}], ",", 
     RowBox[{"AspectRatio", "\[Rule]", "1"}], ",", 
     RowBox[{"AxesLabel", "->", 
      RowBox[{"{", 
       RowBox[{"\"\<Real\>\"", ",", "\"\<Imaginary\>\""}], "}"}]}], ",", 
     RowBox[{"AxesStyle", "\[Rule]", "Gray"}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Medium"}]}], "]"}], 
   "\[IndentingNewLine]"}]}]], "Input",
